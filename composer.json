{
    "name": "hasnayeen/blade-eva-icons",
    "description": "A package to easily make use of Eva Icons in your Laravel Blade views.",
    "keywords": ["Blade", "Eva icons", "Laravel"],
    "homepage": "https://github.com/Hasnayeen/blade-eva-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Nehal Hasnayeen",
            "email": "hasnayeen@pm.me",
            "homepage": "https://hasnayeen.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Hasnayeen\\Evaicons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hasnayeen\\Evaicons\\BladeEvaiconsServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
